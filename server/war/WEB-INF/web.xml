<?xml version="1.0" encoding="UTF-8" standalone="no"?><web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd ">

  <!-- Servlets --><servlet>
  	<servlet-name>hello</servlet-name>
  	<servlet-class>com.appspot.omega.HelloWorld</servlet-class>
</servlet>
	<servlet-mapping>
		<servlet-name>hello</servlet-name>
		<url-pattern>/hello</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>RegionServlet</servlet-name>
		<servlet-class>com.appspot.omega.RegionServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RegionServlet</servlet-name>
		<url-pattern>/RegionServlet</url-pattern>
	</servlet-mapping>

	<!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>web/index.html</welcome-file>
  </welcome-file-list>

</web-app>